{
  "name": "react-pro",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run clean && npm run build-client && npm run build-server",
    "build-client": "NODE_ENV=production BABEL_ENV=production node -r babel-register node_modules/.bin/webpack --progress --display-modules --config webpack/webpack.config.client.js",
    "build-server": "NODE_ENV=production BABEL_ENV=production node -r babel-register node_modules/.bin/webpack --progress --config webpack/webpack.config.server.js",
    "clean": "rimraf dist/ && rimraf public/app",
    "lint": "eslint src",
    "postmerge": "npm install",
    "postrewrite": "npm install",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "start": "npm run clean & NODE_ENV=development BABEL_ENV=development babel-node src/server",
    "start-analyzer": "npm run clean && NODE_ENV=development BABEL_ENV=development ANALYZER=true babel-node src/server",
    "start-production": "NODE_ENV=production BABEL_ENV=production babel-node src/server",
    "test": "echo \"Error: no test specified\"",
    "watch-client": "webpack --watch --colors --config webpack/webpack.config.client.js",
    "watch-server": "webpack --watch --colors --config webpack/webpack.config.server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MilkZoft/react-pro.git"
  },
  "engines": {
    "node": "v8.10.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "author": "Carlos Santana",
  "license": "MIT",
  "dependencies": {
    "express": "^4.15.4",
    "node-time-ago": "^1.0.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^2.0.0",
    "husky": "^0.14.3",
    "isomorphic-style-loader": "^4.0.0",
    "node-sass": "^4.5.3",
    "open": "0.0.5",
    "react-hot-loader": "^4.3.8",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^1.3.3",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  }
}
